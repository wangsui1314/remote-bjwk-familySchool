<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bjwk.dao.LuckyDrawDao">
   
   <insert id="insertLable">
     insert into lable(lable_name) values(#{lableName})
   </insert>
   
   
   <delete id="deleteLable">
     delete from lable where lable_id=#{lableId}
   </delete>
   
   
   <delete id="deleteLableToUserLable">
     delete from user_lable where lable_id=#{lableId}
   </delete>
   
   
   <select id="querLables" resultType="java.util.Map">
   select * from lable
   </select>
</mapper>       